                        <input type="email" class="form-control form-control-lg" placeholder="Email or Mobile No.">                                      
                    </div>
                    <button type="button" class="btn btn-lg btn-primary w-100">
                        <span class="icon-wrap"><img src="../assets/img/101-delivery/icons/voucher.svg" alt=""></span>
                        Get Discount Coupon!</button>
                </form>
            </div>
        </div>
    </div>
</div>

<!-- Modal -->
<div class="modal fade" id="modal-product-item" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false">
<div class="modal-dialog" role="document">
    <div class="modal-content modal-product-item">
    <div class="modal-body">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <i class="fal fa-times"></i>
        </button>
        <!---spinner loading--->
        <div class="overlay-loading d-none">
            <div class="spinner spinner-border text-primary" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>
        <div class="product-item mb-0">
            <div class="product-image">
                <img src="https://via.placeholder.com/500x375" alt="" class="w-100 img-fluid">
            </div>
            <div class="product-content p-3">
                    <div class="product-title">
                            Charcoal Grilled (Shima Hokke)
                    </div>
                    <div class="product-wrap">
                        <div class="product-price">
                            ฿ 329 (A la carte)
                        </div>
                        <div class="order-amount">
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <button class="btn btn-minus"><i class="fas fa-minus"></i></button>
                                </div>
                                <input type="number" value="1" class="form-control" aria-label="amount">
                                <div class="input-group-append">
                                    <button class="btn btn-add"><i class="fas fa-plus"></i></button>
                                </div>
                            </div>
                        </div>
                    </div>
    
                    <div class="product-add-cart mt-3">
                        <button class="btn btn-add-cart btn-style btn-primary text-uppercase p-1">Add to cart</button>
                    </div>
            </div>
            
        </div>
        <div class="product-add-success d-none">
            <div class="content-wrap">
                <div class="text-center icon-wrap"><img src="../assets/img/101-delivery/icons/checked.svg" alt=""></div>
                <div class="text-center text-title mt-3">Add to cart success</div>
            </div>
            <div class="button-group">
                <a href="#" class="btn btn-style btn-primary text-uppercase">Checkout</a>
                <a href="#" class="btn btn-style btn-continue text-hvr-primary text-uppercase text-left"><i class="fas fa-long-arrow-left"></i> Continue ordering</a>
            </div>
        </div>
    </div>
    </div>
</div>
