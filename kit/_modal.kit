
<!-- Welcom Modal -->
<div class="modal fade" id="modal-welcome" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-welcome" role="document">
        <div class="modal-content">
            <div class="modal-body">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true"><i class="fal fa-times"></i></span>
                </button>
                <div class="text-center logo-top"><img src="../assets/img/101-delivery/logo-footer.jpg" alt=""></div>
                <div class="modal-title">
                    <div class="text-label">ENJOY<br>UP TO</div>
                    <div class="text-group">
                        <span class="text-discount">50</span> <span class="text-label-3">%OFF</span>  
                    </div>       
                </div>
                <p class="text-center text-signup">  <a href="#" class="text-signup text-skyblue">+ Free Delivery ! Sign Up Now</a></p>
