<main class="main-content">
  <section class="section section-checkout">
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-lg-8">
          <ul class="checkout-step">
            <li class="step step-mycart active">
              <div class="step-wrap">
                <div class="step-icon"><span class="icon icon-ico-basket"> </span></div>
                <div class="step-title">My cart</div>
              </div>
            </li>
            <li class="step step-summary active">
              <div class="step-wrap">
                <div class="step-icon"><span class="icon icon-ico-summary"></span></div>
                <div class="step-title">Summary</div>
              </div>
            </li>
            <li class="step step-confirmation active">
              <div class="step-wrap">
                <div class="step-icon"><span class="icon icon-ico-check"></span></div>
                <div class="step-title">Confirmation</div>
              </div>
            </li>
          </ul>
        </div>
      </div>
      <div class="checkout-title mt-3 mb-3">
        <div class="mb-3">
          <span class="icon-wrap icon-success"><img src="../assets/img/101-delivery/icons/checkout-check.svg" alt=""></span>
        </div>
        Your order is placed
      </div>
      <div class="checkout-wrap checkout-order-receipt">
        <div class="row justify-content-center">
          <div class="box-wrap">
