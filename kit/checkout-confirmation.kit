                        <div class="col-12 col-md-8">
                          <div class="input-group">
                            <span class="text text-primary mr-3">Coupon code</span>
                            <input type="text" class="form-control" placeholder="Enter your code here">
                          </div>
                        </div>
                        <div class="col-12 col-md-4">
                          <div class="sub-total">
                            <p class="text-primary text-medium mb-0 mr-5">Sub total</p>
                            <span class="text-medium">฿ 1,137</span>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="sub-total row-wrap">
                      <div class="text-wrap">
                        <p class="text-medium text-uppercase">Sub total</p>
                      </div>
                      <div class="text-wrap text-right">
                        <p class="discount-price text-medium"> ฿114</p>
                      </div>
                    </div>
                    <div class="code-discount row-wrap">
                      <div class="text-wrap">
                        <p class="text-medium text-uppercase text-primary text-underline">Voucher discount</p>
                      </div>
                      <div class="text-wrap text-right">
                        <p class="discount-price text-medium text-danger">- ฿114</p>
                      </div>
                    </div>
                    <div class="delivery-charge row-wrap">
                      <div class="text-wrap">
                        <p class="text-medium text-uppercase">Delivery Charge</p>
                      </div>
                      <div class="text-wrap text-right">
                        <p class="text-green text-medium">Free</p>
