<main class="main-content">
  <section class="section section-setting">
    <div class="container">
      <div class="section-title">
        <div class="title">Account Settings</div>
      </div>
      <div class="d-md-flex">
        <div class="sidebar">
          <div class="sidebar-wrap">
            <div class="dropdown">
              <button type="button" class="btn dropdown-toggle" id="select-settings-menu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                My profile
              </button>
              <div class="dropdown-menu" aria-labelledby="select-settings-menu">
                <div class="list-group">
                  <a class="list-group-item list-group-item-action d-none" id="list-overview" data-toggle="list" href="#tab-overview" role="tab" aria-controls="overview">Overview</a>
                  <a class="list-group-item list-group-item-action active" id="list-my-profile" data-toggle="list" href="#tab-my-profile" role="tab" aria-controls="my-profile">Customer Details</a>
